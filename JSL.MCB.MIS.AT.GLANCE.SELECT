SUBROUTINE JSL.MCB.MIS.AT.GLANCE.SELECT
$INSERT I_COMMON
$INSERT I_EQUATE
$INSERT I_JSL.MCB.MIS.AT.GLANCE

     AC.CMD='SELECT ':FN.AC:' WITH (CATEGORY GE "3000" AND CATEGORY LE "3999") OR (CATEGORY GE "6000" AND CATEGORY LE "6999")'
	 CALL EB.READLIST(AC.CMD,AC.LIST,'','','')
	 CALL BATCH.BUILD.LIST('',AC.LIST)
	 RETURN
END